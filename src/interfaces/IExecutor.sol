pragma solidity ^0.8.13;

interface IExecutor {
    function queueReport(
        bool _pending,
        bool _approved,
        uint256 _reportId,
        uint256 _poolId
    ) external;

    function queuePool(
        string memory _protocolName,
        uint256 _proposalId,
        address _protocol,
        uint256 _maxCapacity,
        uint256 _timestamp,
        address _proposer,
        bool _pending,
        bool _approved
    ) external;

    function executeReport(uint256 _reportId) external;

    function executeNewPool(uint256 _proposalId) external;

